{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": "> 1%",
          "uglify": true
        },
        "useBuiltIns": true
      }
    ],
    "react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ],
    ["prismjs", {
        "languages": ["javascript", "css", "markup", "jsx", "haml", "ruby"],
        "plugins": ["line-numbers", "normalize-whitespace"],
        "theme": "default",
        "css": false
    }]

  ]
}
